{
  "name": "epp",
  "productName": "Epp",
  "version": "0.0.0",
  "description": "Electron Application",
  "license": "MIT",
  "repository": "importre/epp",
  "author": {
    "name": "Jaewe Heo",
    "email": "jaeweheo@gmail.com",
    "url": "import.re"
  },
  "engines": {
    "node": ">=4"
  },
  "electronVersion": "0.33.3",
  "scripts": {
    "test": "xo",
    "start": "webpack --config webpack.config.js; electron .",
    "start-dev": "node server.js",
    "start-dev2nd": "node server.js --2nd",
    "start-hot": "webpack-dev-server --config webpack.config.js --hot --port 8000 --inline",
    "build": "npm run build-osx && npm run build-win",
    "build-osx": "webpack --config webpack.config.js; electron-packager . $npm_package_productName --overwrite --out=dist --ignore='^/dist$' --prune --asar --platform=darwin --arch=x64 --version=$npm_package_electronVersion --icon=assets/epp.icns",
    "build-win": "webpack --config webpack.config.js; electron-packager . $npm_package_productName --overwrite --out=dist --ignore='^/dist$' --prune --asar --platform=win32 --arch=all --version=$npm_package_electronVersion --icon=assets/epp.ico"
  },
  "files": [
    "index.js",
    "index.html",
    "dev.html",
    "webpack.config.js",
    "index.scss"
  ],
  "keywords": [
    "electron",
    "webpack",
    "react",
    "bootstrap",
    "electron-app"
  ],
  "dependencies": {
    "bootstrap-sass": "^3.3.5",
    "electron-debug": "^0.2.1",
    "history": "^1.12.3",
    "react": "^0.14.0",
    "react-bootstrap": "^0.27.1",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc3",
    "react-router-bootstrap": "^0.19.1"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "cjsx-loader": "^2.0.1",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "css-loader": "^0.19.0",
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.33.3",
    "file-loader": "^0.8.4",
    "json-loader": "^0.5.3",
    "node-sass": "^3.3.3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "xo": "^0.9.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
